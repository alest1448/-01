    <Grid>
        <Rectangle x:Name="rectangle" Fill="#20B2AA" HorizontalAlignment="Left" Height="120" Margin="60,220,0,0" VerticalAlignment="Top" Width="120" RadiusX="60" RadiusY="60"/>
        <Rectangle x:Name="rectangle1" Fill="#FF00FF" HorizontalAlignment="Left" Height="120" Margin="185,60,0,0" VerticalAlignment="Top" Width="120" RadiusX="60" RadiusY="60"/>
